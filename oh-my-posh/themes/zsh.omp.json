{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "16",
    "white": "231",
    "green": "2",
    "darkestgreen": "22",
    "darkgreen": "28",
    "mediumgreen": "70",
    "brightgreen": "148",
    "darkestcyan": "23",
    "darkcyan": "74",
    "mediumcyan": "117",
    "brightcyan": "159",
    "darkestblue": "24",
    "darkblue": "31",
    "red": "1",
    "darkestred": "52",
    "darkred": "88",
    "mediumred": "124",
    "brightred": "160",
    "brightestred": "196",
    "darkestpurple": "55",
    "mediumpurple": "98",
    "brightpurple": "189",
    "darkorange": "94",
    "mediumorange": "166",
    "brightorange": "208",
    "brightestorange": "214",
    "yellow": "11",
    "brightyellow": "220",
    "gray0": "233",
    "gray1": "235",
    "gray2": "236",
    "gray3": "239",
    "gray4": "240",
    "gray5": "241",
    "gray6": "244",
    "gray7": "245",
    "gray8": "247",
    "gray9": "250",
    "gray10": "252",
    "gray11": "234",
    "gray90": "254",
    "gray70": "249",
    "lightyellowgreen": "106",
    "gold3": "178",
    "orangered": "202",
    "steelblue": "67",
    "darkorange3": "166",
    "skyblue1": "117",
    "khaki1": "228"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "background": "p:brightgreen",
          "background_templates": [
            "{{ if matchP \"main|viins\" .Env.POSH_VI_KEYMAP }}p:brightgreen{{ else }}p:white{{ end }}"
          ],
          "foreground": "p:darkestgreen",
          "style": "diamond",
          "type": "text",
          "template": " <b>{{ if matchP \"main|viins\" .Env.POSH_VI_KEYMAP }}INSERT{{ else }}COMMAND{{ end }}</b> "
        },
        {
          "background": "p:darkblue",
          "foreground": "p:white",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "session",
          "template": " {{ .UserName }}@{{ .HostName }} "
        },
        {
          "background": "p:gray4",
          "foreground": "p:gray9",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "path",
          "properties": {
            "folder_separator_icon": " <p:gray7></> ",
            "style": "agnoster_short",
            "max_depth": 5,
            "right_format": "<p:gray10><b>%s</b></>"
          },
          "template": " {{ if matchP \"^[^/~]\" .Path }}/ <p:gray7></> {{ end }}{{ if matchP \"^[^]+$\" .Path }}<p:gray10><b>{{ .Path }}</b></>{{ else }}{{ .Path }}{{ end }} "
        },
        {
          "background": "p:mediumorange",
          "foreground": "p:brightyellow",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text",
          "template": "{{ if gt (.Env.POSH_JOBNUM | int) 0 }} {{ .Env.POSH_JOBNUM }} {{ end }}"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "background": "p:darkestred",
          "foreground": "p:white",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "type": "status",
          "template": " {{ if gt .Code 0 }}{{ .Code }}{{ end }} "
        },
        {
          "background": "p:gray2",
          "foreground": "p:gray9",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "type": "git",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "branch_icon": "\ue0a0 "
          },
          "template": "{{ if gt .StashCount 0 }} <p:brightyellow>ST {{ .StashCount }}</> {{ if or (.Working.Changed) (.Staging.Changed) }}<p:brightyellow>{{ else }}<p:gray7>{{ end }}</>{{ end }} {{ if or (.Working.Changed) (.Staging.Changed) }}<p:brightyellow>{{ else }}<p:gray9>{{ end }}{{ .HEAD }}</> "
        }
      ]
    }
  ],
  "final_space": true,
  "version": 2
}
